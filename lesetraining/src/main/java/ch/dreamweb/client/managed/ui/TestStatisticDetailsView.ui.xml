<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" xmlns:g="urn:import:com.google.gwt.user.client.ui">
 <ui:style>
    .fields {
      margin-top: 0.5em;
      margin-left: 1em;
    }
    .label {
      min-height: 25px;
      font-weight: bold;
    }
    .button {
      margin-right: 1em;
    }
    .bar {
      margin-left: 1em;
    }
    .header {
      margin-left: 1em;
      color: #4B4A4A;
      text-shadow: #ddf 1px 1px 0;
      margin-bottom: 0;
    }
    .underline {
      border-bottom: 2px solid #6F7277;
    }
  </ui:style>
  <g:HTMLPanel>
    <div class="{style.underline}">
      <h3 class="{style.header}">
                <ui:msg>
                    <span ui:field="displayRenderer"/>
                </ui:msg>
            </h3>
    </div>

    <table class="{style.fields}" id="boundElementHolder">
      
      
      
      
      
      
      
      
      
      
      
      
      
      
    <tr id="id">
                <td>
                    <span class="{style.label}">Id:</span>
                </td>
                <td>
                    <span ui:field="id"/>
                </td>
            </tr>
            <tr id="login">
                <td>
                    <span class="{style.label}">Login:</span>
                </td>
                <td>
                    <span ui:field="login"/>
                </td>
            </tr>
            <tr id="datetime">
                <td>
                    <span class="{style.label}">Datetime:</span>
                </td>
                <td>
                    <span ui:field="datetime"/>
                </td>
            </tr>
            <tr id="test">
                <td>
                    <span class="{style.label}">Test:</span>
                </td>
                <td>
                    <span ui:field="test"/>
                </td>
            </tr>
            <tr id="testround">
                <td>
                    <span class="{style.label}">Testround:</span>
                </td>
                <td>
                    <span ui:field="testround"/>
                </td>
            </tr>
            <tr id="spanscore">
                <td>
                    <span class="{style.label}">Spanscore:</span>
                </td>
                <td>
                    <span ui:field="spanscore"/>
                </td>
            </tr>
            <tr id="trialscore">
                <td>
                    <span class="{style.label}">Trialscore:</span>
                </td>
                <td>
                    <span ui:field="trialscore"/>
                </td>
            </tr>
            <tr id="trialscore_version">
                <td>
                    <span class="{style.label}">Trialscore_version:</span>
                </td>
                <td>
                    <span ui:field="trialscore_version"/>
                </td>
            </tr>
            <tr id="history">
                <td>
                    <span class="{style.label}">History:</span>
                </td>
                <td>
                    <span ui:field="history"/>
                </td>
            </tr>
            <tr id="version">
                <td>
                    <span class="{style.label}">Version:</span>
                </td>
                <td>
                    <span ui:field="version"/>
                </td>
            </tr>
        </table>

  <div class="{style.bar}">
    <g:Button addStyleNames="{style.button}" ui:field="edit">Edit</g:Button>
    <g:Anchor ui:field="delete">Delete</g:Anchor>
   </div>
  </g:HTMLPanel>
</ui:UiBinder>
