<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" xmlns:a="urn:import:ch.dreamweb.client.managed.ui" xmlns:d="urn:import:com.google.gwt.user.datepicker.client" xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:r="urn:import:ch.dreamweb.client.scaffold.ui">
  <ui:style>
    .outer {
      padding: 10px;
    }
    .error {
      padding-left: 0.5em;
      background-color: red;
    }
    .label {
      font-weight: bold;
    }
    .value {
      padding-bottom: 10px;
    }
  </ui:style>
 
  <g:HTMLPanel debugId="boundElementHolder" styleName="{style.outer}">
    <div class="{style.error}" ui:field="errors"/>
    
    
    
    
    
    
    
    
    
    
    
    
    

    <g:Button ui:field="save">Save</g:Button>
  <div id="login">
            <div class="{style.label}">Login</div>
            <div class="{style.value}">
                <g:TextBox ui:field="login"/>
            </div>
        </div>
        <div id="datetime">
            <div class="{style.label}">Datetime</div>
            <div class="{style.value}">
                <d:DateBox ui:field="datetime"/>
            </div>
        </div>
        <div id="test">
            <div class="{style.label}">Test</div>
            <div class="{style.value}">
                <g:IntegerBox ui:field="test"/>
            </div>
        </div>
        <div id="testround">
            <div class="{style.label}">Testround</div>
            <div class="{style.value}">
                <g:IntegerBox ui:field="testround"/>
            </div>
        </div>
        <div id="spanscore">
            <div class="{style.label}">Spanscore</div>
            <div class="{style.value}">
                <g:IntegerBox ui:field="spanscore"/>
            </div>
        </div>
        <div id="trialscore">
            <div class="{style.label}">Trialscore</div>
            <div class="{style.value}">
                <g:IntegerBox ui:field="trialscore"/>
            </div>
        </div>
        <div id="trialscore_version">
            <div class="{style.label}">Trialscore_version</div>
            <div class="{style.value}">
                <g:IntegerBox ui:field="trialscore_version"/>
            </div>
        </div>
        <div id="history">
            <div class="{style.label}">History</div>
            <div class="{style.value}">
                <g:TextBox ui:field="history"/>
            </div>
        </div>
    </g:HTMLPanel>
</ui:UiBinder>
